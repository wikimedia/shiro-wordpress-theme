{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "shiro/tabbed-item",
	"title": "Tabbed Item",
	"parent": [ "shiro/tabbed" ],
	"category": "wikimedia",
	"icon": "minus",
	"description": "Preformatted block grouping for a single tabbed item.",
	"keywords": [ "tabbed-content", "tabs", "tabbed" ],
	"version": "1.0.0",
	"textdomain": "shiro-admin",
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"isFirstChild": {
			"type": "boolean",
			"default": false
		},
		"title": {
			"type": "string",
			"source": "html",
			"selector": "h3"
		}
	},
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css"
}
