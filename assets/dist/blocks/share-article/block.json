{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "shiro/share-article",
  "title": "Share article",
  "category": "wikimedia",
  "icon": "share",
  "description": "Buttons to share an article on Twitter, Facebook, LinkedIn, via Email, or to copy a link.",
  "attributes": {
    "enableTwitter": {
      "type": "boolean",
      "default": true
    },
    "enableBluesky": {
      "type": "boolean",
      "default": true
    },
    "enableFacebook": {
      "type": "boolean",
      "default": true
    },
    "enableLinkedIn": {
      "type": "boolean",
      "default": true
    },
    "enableEmail": {
      "type": "boolean",
      "default": true
    },
    "enableCopyLink": {
      "type": "boolean",
      "default": true
    }
  },
  "example": {},
  "render": "file:/render.php",
  "editorScript": "file:./index.js"
}