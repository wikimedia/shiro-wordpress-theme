		"custom": {
			"fontWeight": {
				"normal": 400,
				"semi-bold": 600,
				"bold": 700
			},
			"lineHeight": {
				"tight": 1.25,
				"snug": 1.333,
				"base": 1.4,
				"relaxed": 1.5
			}
		},
		"layout": {
			"contentSize": "788px",
			"wideSize": "1200px"
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Shadow 16px blur",
					"slug": "shadow-16-blur",
					"shadow": "0 0 var(--wp--preset--spacing--16) 0 rgba(0, 0, 0, 0.25)"
				}
			]
        },
		"spacing": {
			"customSpacingSize": false,
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "8",
					"size": "0.5rem",
					"slug": "8"
				},
				{
					"name": "16",
					"size": "clamp(0.5rem, 0.91vw + 0.32rem, 1rem)",
					"slug": "16"
				},
				{
					"name": "24",
					"size": "clamp(1rem, 0.91vw + 0.82rem, 1.5rem)",
					"slug": "24"
				},
				{
					"name": "32",
					"size": "clamp(1rem, 1.82vw + 0.64rem, 2rem)",
					"slug": "32"
				},
				{
					"name": "40",
					"size": "clamp(1rem, 2.73vw + 0.45rem, 2.5rem)",
					"slug": "40"
				},
				{
					"name": "64",
					"size": "clamp(2rem, 3.64vw + 1.27rem, 4rem)",
					"slug": "64"
				},
				{
					"name": "80",
					"size": "clamp(2.5rem, 4.55vw + 1.59rem, 5rem)",
					"slug": "80"
				}
			],
			"blockGap": true,
			"padding": true,
			"margin": true
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"defaultFontSizes": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "swap",
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "400 700",
							"src": [
								"file:./assets/src/fonts/inter.ttf"
							]
						},
						{
							"fontDisplay": "swap",
							"fontFamily": "Inter",
							"fontStyle": "italic",
							"fontWeight": "400 700",
							"src": [
								"file:./assets/src/fonts/inter-italic.ttf"
							]
						}
					],
					"fontFamily": "Inter, sans-serif",
					"name": "Default (Inter)",
					"slug": "default"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "swap",
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "400 700",
							"src": [
								"file:./assets/src/fonts/montserrat.ttf"
							]
						}
					],
					"fontFamily": "Montserrat, Inter, sans-serif",
					"name": "Display (Montserrat)",
					"slug": "display"
				}
			],
			"fontSizes": [
				{
					"size": "0.75rem",
					"slug": "x-small",
					"name": "Extra Small"
				},
				{
					"size": "0.875rem",
					"slug": "small",
					"name": "Small"
				},
				{
					"size": "1rem",
					"slug": "medium",
					"name": "Medium"
				},
				{
					"fluid": {
						"min": "1.125rem",
						"max": "1.25rem"
					},
					"size": "1.25rem",
					"slug": "large",
					"name": "Large"
				},
				{
					"fluid": {
						"min": "1.25rem",
						"max": "1.5rem"
					},
					"size": "1.5rem",
					"slug": "x-large",
					"name": "Extra Large (Header 3)"
				},
				{
					"fluid": {
						"min": "1.5rem",
						"max": "2rem"
					},
					"size": "2rem",
					"slug": "xx-large",
					"name": "XX Large (Header 2)"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "3rem"
					},
					"size": "3rem",
					"slug": "jumbo",
					"name": "Jumbo (Header 1)"
				}
			],
			"lineHeight": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/navigation": {
				"typography": {
					"fontWeight": "500"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/quote": {
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var:preset|font-size|small"
						},
						"color": {
							"text": "var:preset|color|secondary"
						}
					}
				},
				"css": "& :where(cite) { display: block }",
				"border": {
					"radius": "0",
					"style": "solid",
					"width": "0 0 0 5px !important",
					"color": "var:preset|color|primary"
				},
				"spacing": {
					"padding": {
						"top": "var:preset|spacing|40",
						"bottom": "var:preset|spacing|40"
					}
				},
				"typography": {
					"lineHeight": "var:custom|line-height|body",
					"fontSize": "var:preset|font-size|medium",
					"fontWeight": "500"
				}
			},
			"core/separator": {
				"color": {
					"text": "var:preset|color|main"
				}
			},
			"core/table": {
				"typography": {
					"fontSize": "var:preset|font-size|small"
				}
			}
		},
		"color": {
			"background": "var:preset|color|base",
			"text": "var:preset|color|main"
		},
		"elements": {
			"button": {
				"border": {
					"color": "transparent",
					"radius": "4px",
					"style": "solid",
					"width": "2px"
				},
				"color": {
					"background": "var:preset|color|main",
					"text": "var:preset|color|base"
				},
				"css": "text-underline-offset:3px",
				"spacing": {
					"padding": {
						"top": "0.475em",
						"right": "0.8em",
						"bottom": "0.475em",
						"left": "0.8em"
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|large",
					"fontWeight": "var:custom|font-weight|bold",
					"lineHeight": "var:custom|line-height|tight"
				},
				":hover": {
					"border": {
						"color": "var:preset|color|main"
					},
					"color": {
						"background": "var:preset|color|border-light",
						"text": "var:preset|color|main"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":active": {
					"border": {
						"color": "var:preset|color|main"
					},
					"color": {
						"background": "var:preset|color|border-base",
						"text": "var:preset|color|main"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":focus": {
					"border": {
						"color": "var:preset|color|main"
					},
					"color": {
						"background": "var:preset|color|border-light",
						"text": "var:preset|color|main"
					},
					"outline": {
						"offset": "2px",
						"width": "2px",
						"style": "solid",
						"color": "var:preset|color|main"
					},
					"typography": {
						"textDecoration": "underline"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|jumbo",
					"lineHeight": "var:custom|line-height|snug"
        }
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|xx-large",
					"lineHeight": "var:custom|line-height|tight"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|x-large",
					"lineHeight": "var:custom|line-height|snug"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|x-large",
					"fontWeight": "var:custom|font-weight|semi-bold"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|x-large",
					"fontWeight": "var:custom|font-weight|normal"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|large"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|display",
					"fontWeight": "var:custom|font-weight|bold"
				}
			},
			"link": {
				"color": {
					"text": "var:preset|color|main"
				}
			}
		},
		"spacing": {
			"blockGap": "var:preset|spacing|40",
			"padding": {
				"top": "0",
				"right": "var:preset|spacing|24",
				"bottom": "0",
				"left": "var:preset|spacing|24"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|default",
			"fontSize": "var:preset|font-size|medium",
			"fontWeight": "var:custom|font-weight|normal",
			"lineHeight": "var:custom|line-height|base"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "mega-get-involved",
			"title": "Mega get involved",
			"area": "menu"
		},
		{
			"name": "mega-languages",
			"title": "Mega languages",
			"area": "menu"
		},
		{
			"name": "mega-news",
			"title": "Mega news",
			"area": "menu"
		},
		{
			"name": "mega-search",
			"title": "Mega search",
			"area": "menu"
		},
		{
			"name": "mega-what-we-do",
			"title": "Mega what we do",
			"area": "menu"
		},
		{
			"name": "mega-who-we-are",
			"title": "Mega who we are",
			"area": "menu"
		}
	]
}
